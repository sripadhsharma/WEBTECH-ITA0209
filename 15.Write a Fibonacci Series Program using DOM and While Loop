<html>
<body>
     <script type="text/javascript">
          var no1=0,no2=1,no3=0;
          document.write("Fibonacci Series:"+"<br>");
          while (no2<=10)
          {
               no3 = no1+no2;
               no1 = no2;
               no2 = no3;
               document.write(no3+"<br>");
          }
     </script>
</body>
</html>
